        function ObtenerColaborador(CodigoAuto) {
            var NombresApellidos = $('#txtApellidosNombres').val();
            var Documento = $('#txtDocumento').val();
            var Celular = $('#txtCelular').val();
            var Anexo = $('#txtAnexo').val();



            var oBColaborador = {};
            oBColaborador.CodigoAuto = sessionStorage.CodigoAuto;
            oBColaborador.NombresApellidos = NombresApellidos;
            oBColaborador.Documento = Documento;
            oBColaborador.Celular = Celular;
            oBColaborador.Anexo = Anexo;



            $.ajax({
                url: sessionStorage.URL + '/Formulario/ObtenerColaborador',
                type: "POST",
                dataType: "json",
                contentType: "application/json; charset=utf-8",
                processData: true,
                data: JSON.stringify(oBColaborador),
                success: function (response) {
                    console.log(response);
                    if (response.Result) {
                        if (response.Data.EsColaborador == 1) {
                            $("#txtApellidosNombres").val(response.Data.NombresApellidos);
                            document.getElementById('txtApellidosNombres').disabled = true;
                            $("#txtDocumento").val(response.Data.Documento);
                            document.getElementById('txtDocumento').disabled = true;
                            $("#txtCelular").val(response.Data.Celular);
                            $("#txtAnexo").val(response.Data.Anexo);
                            document.getElementById('#txtAnexo');
                            puestoColaborador = response.Data.Puesto;
                            areaColaborador = response.Data.Area;
                            response.Data.CodigoAuto = sessionStorage.CodigoAuto;

                        } else {
                            if (response.Data.NombresApellidos != "") {
                                $("#txtApellidosNombres").val(response.Data.NombresApellidos);
                                document.getElementById('txtApellidosNombres').disabled = true;
                                $("#txtDocumento").val(response.Data.Documento);
                                $("#txtCelular").val(response.Data.Celular);
                                $("#txtAnexo").val(response.Data.Anexo);


                            }

                        }

                    } else {

                    }
                    $.unblockUI();
                },

                failure: function (msg) {
                    console.log(msg);
                    $.unblockUI();
                },

                error: function (xhr, status, error) {
                    console.log(error);
                    $.unblockUI();
                },

                complete: function () {
                    //$.unblockUI();
                    //$("#prueba").dialog("close")
                }
            });
        }

